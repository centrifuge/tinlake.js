{
  "name": "tinlake",
  "version": "0.0.16",
  "description": "Centrifuge Tinlake Contracts Client",
  "main": "dist/Tinlake.js",
  "types": "dist/Tinlake.d.ts",
  "module": "dist/Tinlake.es.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "decimal.js-light": "^2.5.0",
    "ethers": "^4.0.45",
    "ethjs": "^0.4.0",
    "ethjs-account": "^0.1.4",
    "ethjs-provider-signer": "^0.1.4",
    "ethjs-signer": "^0.1.1",
    "mocha": "^6.1.4",
    "randomstring": "^1.1.5",
    "web3-eth-abi": "^1.2.6",
    "web3-utils": "^1.2.0"
  },
  "devDependencies": {
    "@types/assert": "^1.4.2",
    "@types/bn.js": "^4.11.5",
    "@types/ethjs-signer": "^0.1.0",
    "@types/mocha": "^5.2.7",
    "@types/web3-eth-abi": "^1.0.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "nodemon": "^1.19.1",
    "rollup": "^1.16.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-mocha": "^6.0.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "ts-mocha -p test/tsconfig.json src/**/*.spec.ts --timeout 20000",
    "test-integration": "npm run build && ts-mocha -p test/tsconfig.json test/**/*",
    "nodemon": "nodemon node inspect dist/Tinlake.js",
    "generate-docs": "typedoc --out docs --exclude \"./node_modules/**\"  --exclude \"./src/actions/*.spec.ts\" --excludeExternals --excludeNotExported --ignoreCompilerErrors ./src",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/centrifuge/tinlake.js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/centrifuge/tinlake.js/issues"
  },
  "homepage": "https://github.com/centrifuge/tinlake.js#readme"
